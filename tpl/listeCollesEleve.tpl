<table style="text-align: left; max-width: 200%;" border="1" cellpadding="2" cellspacing="2">
	<tbody>
		<tr class='header' style="text-align: center">
			<th style="min-width:5em;">Jour</th>
			<th style="min-width:4em;">Debut</th>
			<th style="min-width:4em;">Fin</th>
			<th style="min-width:4em;">Matiere</th>
			<th style="min-width:5em;">Colleur</th>
			<th style="min-width:4em;">Lieu</th>
		</tr>
		<!-- BEGIN ligne -->
		<tr class='{ligne.eo}'>
			<td>{ligne.nJour}</td>
			<td>{ligne.Debut}</td>
			<td>{ligne.Fin}</td>
			<td>{ligne.Matiere}</td>
			<td>{ligne.Colleur}</td>
			<td>{ligne.Lieu}</td>
		</tr>
		<!-- END ligne -->
	</tbody>
</table>
